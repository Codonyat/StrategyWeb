# Strategy Coin Contract Configuration
# Copy this file to .env and fill in your values

# Contract address (deployed Strategy.sol address)
VITE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Chain ID - This determines all network-specific settings
# Monad Testnet: 10143
# Monad Mainnet: 143
VITE_CHAIN_ID=10143

# RPC URL for the blockchain network (optional - will use network default if not specified)
# VITE_RPC_URL=https://rpc.monad.xyz

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
# Required for RainbowKit to work properly
VITE_WALLETCONNECT_PROJECT_ID=YOUR_PROJECT_ID
